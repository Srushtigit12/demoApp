<mvc:View controllerName="demo.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    xmlns:chart="sap.suite.ui.microchart"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}" titleAlignment="Center">
    <GenericTile header="Open" class="sapUiTinyMargin"  >
                            <TileContent>
                                
                                    <!-- Numeric value -->
                                    <NumericContent value="1000" truncateValueTo="6" withMargin="false" valueColor="Good">
                                        <layoutData>
                                            <!-- Allow number to consume available space -->
                                            <FlexItemData growFactor="1" />
                                        </layoutData>
                                    </NumericContent>
 
                                    <!-- Radial chart -->
                                  
                                 
 
                                    <!-- Radial chart -->
                                 
 
                              
                            </TileContent>
                        </GenericTile>
    	<f:SimpleForm layout="ResponsiveGridLayout" labelSpanXL="5" labelSpanL="5" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false"
				emptySpanXL="1" emptySpanL="1" emptySpanM="0" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false">
				<f:content>
                <Label text="Claim Type" design="Bold"/>
                <ComboBox selectedKey="{worklistView>/sKey}">
                  <core:Item key="1" text="STD" />
                  <core:Item key="2" text="LTD" />
                  <core:Item key="3" text="Unpaid Medical Leaves" />
                </ComboBox>
                <Label text="Sub Type" design="Bold"/>
                <Input value="" editable="{=${worklistView>/sKey} !== '' ? true: false}"/>
                </f:content>
                </f:SimpleForm>
    </Page>
</mvc:View>